{
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/$1.html",
      "headers": { "source": "/" }
    },
    {
      "src": "/servicos",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/404",
      "dest": "/404/index.html"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404"
    }
  ]
}